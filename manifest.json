{
    "manifest_version": 2,
    "name": "The Ultimate Discord Extension",
    "short_name": "Discord Styling",
    "version": "1.0.0",
    "description": "This extension allows you to personalize your Discord exprience!",
    "icons": {
        "16": "/icon16.png",
        "32": "/icon32.png",
        "48": "/icon48.png",
        "128": "/icon128.png",
        "256": "/icon256.png"
    },
    "browser_action": {
        "name": "The Ultimate Discord Extension",
        "default_icon": "/icon128.png"
    },
    "content_scripts": [{
        "css": ["style.css"],
        "matches": ["https://discordapp.com/*"]
    }]
}
